{
    "presets": [
        [
            "env",
            {
                "target": {
                    "node": "8.11.1"
                },
                "useBuiltIns": true
            }
        ]
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": true,
                "polyfill": true,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}